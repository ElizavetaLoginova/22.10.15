#include <iostream>
using namespace std;
int main()
{
	while (true)
	{
		long long n;
		cout << "Enter your number: " << endl;
		cin >> n;

		int m;
		cout << "Enter the discharge (10, 100, 1000 etc.): " << endl;
		cin >> m;
		int k = m, i = 0;
		while (k)
		{
			int a = k % 10;
			if (a == 0)
			{
				i++;	
			}
			k = k / 10;
		}
		long long p = 0, t = abs(n), r = abs(n);
		for (int j = 1; j <= i; j++)
		p = t / 10;
		for (int j = 1; j <= i; j++)
		r = t % 100 - t % 10;
		if (r >= 5)
			p++;
		cout << "Result is : " << p << endl;
	}

	system("pause");
	system("cls");
	return 0;
}
